{
  "name": "point",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "yarn run dc:dev:db:up",
    "dev": " trap '' EXIT INT TERM; dotenv -e .env.dev next dev",
    "postdev": " yarn run  dc:dev:db:down",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:dev": "dotenv -e .env.dev prisma migrate dev",
    "postinstall": "prisma generate",
    "dc:dev:up": "sudo docker-compose -f docker/dev/docker-compose.yml -p point-dev up -d",
    "dc:dev:down": "sudo docker-compose -f docker/dev/docker-compose.yml -p point-dev  down",
    "dc:dev:db:up": "sudo docker-compose -f docker/dev/docker-compose.yml -p point-dev up -d db",
    "dc:dev:db:down": "sudo docker-compose -f docker/dev/docker-compose.yml -p point-dev  down  "
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.12.0",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^4.1.1",
    "@trpc/client": "^9.26.2",
    "@trpc/next": "^9.26.2",
    "@trpc/react": "^9.26.2",
    "@trpc/server": "^9.26.2",
    "@types/cytoscape": "^3.19.7",
    "@types/react-cytoscapejs": "^1.2.2",
    "cytoscape": "^3.22.1",
    "next": "12.2.3",
    "next-auth": "^4.10.3",
    "prisma-generator-trpc": "^0.0.1",
    "react": "18.2.0",
    "react-cytoscapejs": "^1.2.1",
    "react-dom": "18.2.0",
    "react-query": "^3.39.2",
    "superjson": "^1.9.1",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "autoprefixer": "^10.4.8",
    "dotenv": "^16.0.2",
    "dotenv-cli": "^6.0.0",
    "eslint": "8.18.0",
    "eslint-config-next": "12.2.3",
    "postcss": "^8.4.14",
    "prisma": "^4.1.1",
    "tailwindcss": "^3.1.8",
    "typescript": "4.7.4"
  }
}
